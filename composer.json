{
  "name": "team-nifty-gmbh/livewire-calendar",
  "description": "Laravel Livewire calendar component",
  "type": "library",
  "require": {
    "php": "^8.1",
    "livewire/livewire": "^2.10",
    "illuminate/support": "^9.20",
    "illuminate/database": "^9.20",
    "illuminate/view": "^9.20",
    "wireui/wireui": "^1.3"
  },
  "require-dev": {
    "laravel/pint": "^1.1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "TeamNiftyGmbh\\LivewireCalendar\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Patrick Weh",
      "email": "patrick.weh@team-nifty.com"
    },
    {
      "name": "Martin Slupski",
      "email": "mail@slupi.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "TeamNiftyGmbH\\LivewireCalendar\\LivewireCalendarServiceProvider"
      ],
      "aliases": {
        "LivewireCalendar": "TeamNiftyGmbH\\LivewireCalendar\\LivewireCalendarFacade"
      }
    }
  }
}
